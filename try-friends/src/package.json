{
  "name": "try-friends",
  "version": "1.0.0",
  "description": "Web app to try friends-lang.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "npm run watch",
    "prod": "run-s test build",
    "watch": "run-p watch:*",
    "watch:webpack": "cross-env NODE_ENV=development webpack --watch",
    "watch:webpack-dev-server": "cross-env NODE_ENV=development webpack-dev-server --watch",
    "watch:karma": "cross-env NODE_ENV=development karma start",
    "build": "cross-env NODE_ENV=production webpack",
    "test": "run-s test:build test:karma",
    "test:build": "cross-env NODE_ENV=development webpack",
    "test:karma": "cross-env NODE_ENV=development karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vain0x/friends-lang.git"
  },
  "author": "vain0x",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vain0x/friends-lang/issues"
  },
  "homepage": "https://github.com/vain0x/friends-lang#readme",
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.3",
    "assert-diff": "^2.0.3",
    "awesome-typescript-loader": "^5.2.1",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "mocha-reporter": "^0.1.1",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  }
}
